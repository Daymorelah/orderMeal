{
  "name": "ordermeal",
  "version": "1.0.0",
  "description": "A food-ordering service app for a restaurant",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/app.js --exec babel-node",
    "build": "babel server/ -d dist",
    "host-on-heroku": "npm run build && node dist/app.js",
    "test": "export NODE_ENV=test&& npm run models-for-testing && mocha --exit --require babel-register --require babel-polyfill server/Tests/index.js",
    "test-coverage": "export NODE_ENV=test&& npm run models-for-testing && babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --exit server/Tests/index.js",
    "coveralls": "npm run test-coverage && cat ./coverage/lcov.info | coveralls",
    "models": "babel-node server/Model",
    "models-for-testing": "export NODE_ENV=test&& babel-node server/Model"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daymorelah/orderMeal.git"
  },
  "author": "Ademola Hussain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Daymorelah/orderMeal/issues"
  },
  "homepage": "https://github.com/Daymorelah/orderMeal#readme",
  "dependencies": {
    "babel-istanbul": "^0.12.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsend": "^1.0.2",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.1",
    "pg": "^7.4.3",
    "swagger-ui-dist": "^3.19.1",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4"
  }
}
