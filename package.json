{
  "name": "ordermeal",
  "version": "1.0.0",
  "description": "A food-ordering service app for a restaurant",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/app.js --exec babel-node",
    "build": "babel server/ -d dist",
    "host-on-heroku": "npm run build && node dist/app.js",
    "test": "export NODE_ENV=test&& yarn models-for-testing && mocha --exit --require @babel/register --require @babel/polyfill server/Tests/index.js",
    "test-coverage": "export NODE_ENV=test&& yarn models-for-testing && nyc mocha --exit --require @babel/register --require @babel/polyfill server/Tests/index.js",
    "coveralls": "yarn test-coverage && cat ./coverage/lcov.info | coveralls",
    "models": "babel-node server/Model",
    "models-for-testing": "export NODE_ENV=test&& babel-node server/Model"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daymorelah/orderMeal.git"
  },
  "author": "Ademola Hussain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Daymorelah/orderMeal/issues"
  },
  "homepage": "https://github.com/Daymorelah/orderMeal#readme",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.16.2",
    "jsend": "^1.0.2",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.4.3",
    "swagger-ui-dist": "^3.19.1",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.1.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "^7.4.5",
    "babel-istanbul": "^0.12.2",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2"
  }
}
